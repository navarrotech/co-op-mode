{
  "name": "co-op-mode-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "proto": "pbjs -t static-module --es6 --no-beautify --no-comments --keep-case -w es6 -o ./src/modules/protobuf/schema.js ../schema.proto && pbts -o ./src/modules/protobuf/schema.d.ts ./src/modules/protobuf/schema.js",
    "deploy:production": "firebase deploy",
    "deploy:staging": "firebase hosting:channel:deploy staging",
    "analyze": "vite-bundle-analyzer"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@react-keycloak/web": "^3.4.0",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "bulma": "^1.0.1",
    "bulma-divider": "^0.2.0",
    "firebase": "^10.12.2",
    "google-protobuf": "^3.21.2",
    "i18next": "^23.11.5",
    "i18next-http-backend": "^2.5.2",
    "keycloak-js": "^24.0.5",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-password-strength-bar": "^0.4.1",
    "react-redux": "^9.1.2",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "redux": "^5.0.1",
    "sass": "^1.77.4",
    "storybook": "^8.1.6",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "protobufjs-cli": "^1.1.2",
    "protoc-gen-ts": "^0.8.7",
    "tslib": "^2.6.3",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.2.0",
    "vite-bundle-analyzer": "^0.9.4",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "web-vitals": "^4.0.1"
  }
}
