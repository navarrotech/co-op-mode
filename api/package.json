{
  "name": "volcano-streaming",
  "version": "1.0.0",
  "main": "./index.ts",
  "private": true,
  "author": {
    "name": "Alex Navarro",
    "email": "alex@navarrotech.net"
  },
  "license": "MIT",
  "scripts": {
    "start": "prisma migrate deploy && ts-node -r tsconfig-paths/register ./index.ts",
    "dev::migrate": "prisma generate && prisma db push",
    "dev": "yarn install && nodemon --legacy-watch --exec 'ts-node -r tsconfig-paths/register' ./index.ts",
    "proto": "pbjs -t static-module --keep-case -w commonjs -o ./src/lib/generated/schema.js ../schema.proto && pbts -o ./src/lib/generated/schema.d.ts ./src/lib/generated/schema.js"
  },
  "_moduleAliases": {
    "database": "./lib/database",
    "types": "./lib/types"
  },
  "imports": {
    "lib/*.ts": "./*.ts"
  },
  "dependencies": {
    "@prisma/client": "5.5.2",
    "connect-redis": "^7.1.1",
    "i18n": "^0.15.1",
    "moment": "^2.29.4",
    "navarrotech-express": "^1.0.30",
    "password-hash": "^1.2.2",
    "prisma": "^5.6.0",
    "protobufjs": "^7.3.0",
    "redis": "^4.6.14",
    "uuid": "^9.0.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.12",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.10.3",
    "@types/password-hash": "^1.2.24",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.1.0",
    "protobufjs-cli": "^1.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.8.3",
    "typescript": "^5.3.2"
  }
}
