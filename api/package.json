{
  "name": "volcano-streaming",
  "version": "1.0.0",
  "main": "./index.ts",
  "private": true,
  "author": {
    "name": "Alex Navarro",
    "email": "alex@navarrotech.net"
  },
  "license": "MIT",
  "scripts": {
    "start": "prisma migrate deploy && ts-node -r tsconfig-paths/register ./index.ts",
    "dev:migrate": "prisma generate && prisma db push",
    "dev:proto": "nodemon --legacy-watch --watch ./schema.proto -e proto --exec 'bash ./scripts/proto.sh'",
    "dev:api": "nodemon --legacy-watch --exec 'ts-node -r tsconfig-paths/register' ./index.ts",
    "dev": "concurrently \"yarn run dev:api\" \"yarn run dev:proto\"",
    "proto": "./scripts/proto.sh"
  },
  "_moduleAliases": {
    "database": "./lib/database",
    "types": "./lib/types"
  },
  "imports": {
    "lib/*.ts": "./*.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.22",
    "@grpc/proto-loader": "^0.7.13",
    "@prisma/client": "5.5.2",
    "@types/lodash-es": "^4.17.12",
    "connect-redis": "^7.1.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "navarrotech-express": "^1.0.30",
    "password-hash": "^1.2.2",
    "prisma": "^5.6.0",
    "redis": "^4.6.14",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "uuid": "^9.0.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.10.3",
    "@types/password-hash": "^1.2.24",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "grpc-tools": "^1.12.4",
    "nodemon": "^3.1.0",
    "ts-protoc-gen": "^0.15.0",
    "type-fest": "^4.8.3"
  }
}
