{
  "name": "volcano-streaming",
  "version": "1.0.0",
  "main": "./index.ts",
  "private": true,
  "author": {
    "name": "Alex Navarro",
    "email": "alex@navarrotech.net"
  },
  "license": "MIT",
  "scripts": {
    "start": "prisma migrate deploy && ts-node -r tsconfig-paths/register ./index.ts",
    "dev::migrate": "prisma generate && prisma db push",
    "dev": "yarn install && nodemon --legacy-watch --exec 'ts-node -r tsconfig-paths/register' ./index.ts",
    "proto": "pbjs -t static-module --keep-case -w commonjs -o ./src/lib/generated/schema.js ../schema.proto && pbts -o ./src/lib/generated/schema.d.ts ./src/lib/generated/schema.js",
    "docs": "ts-node --project tsconfig.json -r tsconfig-paths/register src/docs/index.ts",
    "frontend_api": "ts-node --project tsconfig.json -r tsconfig-paths/register src/scripts/generate_api_routes.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "frontend_api:dev": "nodemon --legacy-watch --exec 'ts-node --project tsconfig.json -r tsconfig-paths/register' src/scripts/generate_api_routes.ts"
  },
  "_moduleAliases": {
    "database": "./lib/database",
    "types": "./lib/types"
  },
  "imports": {
    "lib/*.ts": "./*.ts"
  },
  "dependencies": {
    "@prisma/client": "5.5.2",
    "amqplib": "^0.10.4",
    "axios": "^1.7.2",
    "connect-pg-simple": "^9.0.1",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "i18n": "^0.15.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "plivo": "^4.67.0",
    "prisma": "^5.6.0",
    "protobufjs": "^7.3.0",
    "redis": "^4.6.14",
    "uuid": "^9.0.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^2.3.0",
    "@types/amqplib": "^0.10.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/html-minifier": "^4.0.5",
    "@types/i18n": "^0.13.12",
    "@types/lodash": "^4.17.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.10.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-plugin-header": "^3.1.1",
    "html-minifier": "^4.0.0",
    "nodemon": "^3.1.0",
    "protobufjs-cli": "^1.1.2",
    "squirrelly": "^9.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.8.3",
    "typescript": "^5.3.2"
  }
}
