{
  "name": "gateway",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Alex Navarro",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node ./index.ts",
    "dev": "yarn install && nodemon --legacy-watch --exec 'ts-node' ./index.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run --coverage",
    "test:dev": "DEBUG_PRINT_LIMIT=100000 vites watch --ui --coverage.enabled=true",
    "proto": "pbjs -t static-module --keep-case -w commonjs -o ./src/lib/generated/schema.js ../schema.proto && pbts -o ./src/lib/generated/schema.d.ts ./src/lib/generated/schema.js"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^2.3.0",
    "@types/amqplib": "^0.10.5",
    "@types/express-session": "^1.18.0",
    "@types/express-socket.io-session": "^1.3.9",
    "@types/node": "^20.14.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-header": "^3.1.1",
    "nodemon": "^3.1.3",
    "protobufjs-cli": "^1.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@prisma/client": "5.15.0",
    "amqplib": "^0.10.4",
    "connect-redis": "^7.1.1",
    "express-session": "^1.18.0",
    "express-socket.io-session": "^1.3.5",
    "prisma": "^5.15.0",
    "protobufjs": "^7.3.2",
    "redis": "^4.6.14",
    "socket.io": "^4.7.5",
    "uuid": "^10.0.0",
    "vitest": "^1.6.0"
  }
}
